{
    "name": "jmf/crud-engine-bundle",
    "description" : "CRUD engine bundle for Symfony",
    "license": "mit",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "doctrine/orm": "^2.0",
        "symfony/form": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/http-foundation": "^6.0",
        "symfony/routing": "^6.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phing/phing": "^2.17",
        "phpmd/phpmd": "^2.13",
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Jmf\\CrudEngine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jmf\\CrudEngine\\Tests\\": "tests"
        }
    }
}
