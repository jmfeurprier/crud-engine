{
    "name": "jmf/crud-engine",
    "description" : "CRUD engine for Symfony",
    "license" : "mit",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "doctrine/orm": "^2.13",
        "symfony/form": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/routing": "^5.4",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phing/phing": "^2.17",
        "phpmd/phpmd": "^2.10",
        "phpunit/phpunit": "^9.5",
        "sebastian/phpcpd": "^6.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Jmf\\CrudEngine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jmf\\CrudEngine\\Tests\\": "tests"
        }
    }
}
